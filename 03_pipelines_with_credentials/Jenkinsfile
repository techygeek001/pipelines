node {
    ansiColor('gnome-terminal'){
        ws {
            deleteDir()
            stage('print key') {
                withCredentials([sshUserPrivateKey(credentialsId: 'gitlab_key', keyFileVariable: 'ssh_key', passphraseVariable: '', usernameVariable: '')]) {
                    sh 'echo "$ssh_key"'
                }
            }
        }
    }
}